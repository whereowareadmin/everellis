    <item>
      <title><![CDATA[Block Home 9 Slide]]></title>
      <identifier><![CDATA[block_home9_slide]]></identifier>
      <content><![CDATA[<div class="vicomage-slider  vicomage-slider-custom" id="vicohome9-slide">
	<div class="item">
		<a href="#">
			<img class="img-responsive" data-lazy="{{media url="wysiwyg/vicomage/static/home9/h9-slide1.jpg"}}" alt=""/>
		</a>
	</div>
	<div class="item">
		<a href="#">
			<img class="img-responsive" data-lazy="{{media url="wysiwyg/vicomage/static/home9/h9-slide2.jpg"}}" alt=""/>
		</a>
	</div>
	<div class="item">
		<a href="#">
			<img class="img-responsive" data-lazy="{{media url="wysiwyg/vicomage/static/home9/h9-slide3.jpg"}}" alt=""/>
		</a>
	</div>
	<div class="item">
		<a href="#">
			<img class="img-responsive" data-lazy="{{media url="wysiwyg/vicomage/static/home9/h9-slide1.jpg"}}" alt=""/>
		</a>
	</div>
</div>
<script type="text/javascript">
	require([
		'jquery',
		'slick'
	], function ($) {
		var banner_slick = $("#vicohome9-slide");
		banner_slick.slick({
			slidesToShow: 3,
			autoplay: false,
			pauseOnHover: false,
			dots: true,
			lazyLoad: 'progressive',
			arrows: true,
			infinite: false,
			slidesToScroll: 1,
			responsive:
			[
				{'breakpoint': 1201, 'settings': {'slidesToShow': 3}},
				{'breakpoint': 1200, 'settings': {'slidesToShow': 3}},
				{'breakpoint': 992, 'settings': {'slidesToShow': 3}},
				{'breakpoint': 769, 'settings': {'slidesToShow': 2}},
				{'breakpoint': 641, 'settings': {'slidesToShow': 2}},
				{'breakpoint': 481, 'settings': {'slidesToShow': 1}},
				{'breakpoint': 361, 'settings': {'slidesToShow': 1}},
				{'breakpoint': 1, 'settings': {'slidesToShow': 1}}
			]
		});
	});
</script>]]></content>
